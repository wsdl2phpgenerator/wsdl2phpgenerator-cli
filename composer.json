{
    "name": "wsdl2phpgenerator/wsdl2phpgenerator-cli",
    "description": "Simple CLI utility for generating php classes from a wsdl file.",
    "keywords": ["soap", "wsdl"],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "Wsdl2PhpGenerator\\": ["src/"]
        }
    },
    "bin": ["wsdl2php"],
    "require": {
        "php": ">=5.3.0",
        "ext-soap": "*",
        "wsdl2phpgenerator/wsdl2phpgenerator": "3.*",
        "psr/log": "1.0.*",
        "symfony/console": "~2.3"
    },
    "require-dev": {
        "kasperg/phing-github": "0.2.*",
        "kherge/box": "2.4.*",
        "phing/phing": "2.7.*",
        "php-vcr/phpunit-testlistener-vcr": "1.1.*",
        "phpunit/phpunit": "4.0.*"
    }
}
