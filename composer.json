{
    "name": "wsdl2phpgenerator/wsdl2phpgenerator-cli",
    "description": "Simple CLI utility for generating php classes from a wsdl file.",
    "keywords": ["soap", "wsdl"],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "Wsdl2PhpGenerator\\": ["src/", "tests/"]
        }
    },
    "bin": ["wsdl2php"],
    "require": {
        "php": ">=5.3.3",
        "ext-soap": "*",
        "wsdl2phpgenerator/wsdl2phpgenerator": "^3.4",
        "psr/log": "^1.0",
        "symfony/console": "~2.6|~3.4",
        "symfony/options-resolver": "~2.6|~3.0"
    },
    "require-dev": {
        "kasperg/phing-github": "0.2.*",
        "kherge/box": "^2.7",
        "phing/phing": "~2.7",
        "phpunit/phpunit": "~4.4",
        "doctrine/annotations": "1.2.*"
    }
}
